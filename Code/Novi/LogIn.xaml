<Window x:Class="ProjekatSIMS.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjekatSIMS"
        mc:Ignorable="d"
        Title="LogIn" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="324,108,343,304">
            <ComboBox x:Name="Combo">
                <ComboBoxItem IsSelected="True" Content="Patient"/>
                <ComboBoxItem Content="Doctor"/>
                <ComboBoxItem Content="Dean"/>
                <ComboBoxItem Content="Secretary"/>
            </ComboBox>
        </StackPanel>
        <TextBox x:Name="TBEmail" HorizontalAlignment="Left" Margin="324,173,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="133" Height="28"/>
        <TextBox x:Name="TBPass" HorizontalAlignment="Left" Margin="324,231,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="133" Height="28"/>
        <Label Content="Email:" HorizontalAlignment="Left" Margin="229,175,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="218,233,0,0" VerticalAlignment="Top"/>
        <Button Content="LogIn" HorizontalAlignment="Left" Margin="350,324,0,0" VerticalAlignment="Top" Width="82" Height="35" Click="Button_Click"/>

    </Grid>
</Window>
